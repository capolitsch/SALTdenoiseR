citHeader("To cite the trendfiltering package in publications use:")

note <- sprintf("R package version %s", meta$Version)

citEntry(entry = "Misc",
         author = "Collin A. Politsch",
         title = "SALTdenoiseR: Spectroscopic denoising for the SALT Observatory",
         year = "2021",
         url = "https://capolitsch.github.io/SALTdenoiseR/",
         textVersion = "C. A. Politsch (2021). SALTdenoiseR: Spectroscopic denoising for the SALT Observatory. R package version 0.0.1.",
         note = note,
         key = "SALTdenoiseR")
